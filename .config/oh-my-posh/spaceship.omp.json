{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "green",
          "options": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#7FD5EA",
          "template": " <b></b> {{ .Full }}"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#699E5B",
          "template": "<b></b> {{ .Full }}"
        },
        {
          "type": "nim",
          "style": "plain",
          "foreground": "#f3d400",
          "template": " <b></b> {{ .Full }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#3970A0",
          "template": " <b></b> {{ .Full }}"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#E95914",
          "template": " <b></b> {{ .Full }}"
        },
        {
          "type": "zig",
          "style": "plain",
          "foreground": "#ffad55",
          "template": " <b></b> {{ .Full }}"
        },
        {
          "foreground": "#ff94df",
          "options": {
            "branch_icon": " <#ff94df><b>\uf126 </b></>"
          },
          "style": "plain",
          "template": " <#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "white",
          "options": {
            "always_enabled": true,
            "style": "round"
          },
          "style": "plain",
          "template": "<#373D45>{{ .FormattedMs }}</>",
          "type": "executiontime"
        },
        {
          "foreground": "green",
          "foreground_templates": ["{{ if gt .Code 0 }}red{{ end }}"],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " <b>{{ if gt .Code 0 }}✘{{else}}✔{{ end }}</> ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
